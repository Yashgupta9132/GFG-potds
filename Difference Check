// Java 

import java.util.*;

class Solution {
    public int minDifference(String[] arr) {
        int n = arr.length;
        int[] secondsArray = new int[n];

        for (int i = 0; i < n; i++) {
            secondsArray[i] = timeToSeconds(arr[i]);
        }

        Arrays.sort(secondsArray);

        int minDiff = Integer.MAX_VALUE;

        for (int i = 1; i < n; i++) {
            minDiff = Math.min(minDiff, secondsArray[i] - secondsArray[i - 1]);
        }

        int wrapAroundDiff = 86400 - secondsArray[n - 1] + secondsArray[0];
        minDiff = Math.min(minDiff, wrapAroundDiff);

        return minDiff;
    }

    private int timeToSeconds(String time) {
        int h = Integer.parseInt(time.substring(0, 2));
        int m = Integer.parseInt(time.substring(3, 5));
        int s = Integer.parseInt(time.substring(6, 8));
        return h * 3600 + m * 60 + s;
    }
}
